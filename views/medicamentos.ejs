<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Listado de Medicamentos</title>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    th, td {
      border: 1px solid black;
      padding: 8px;
      text-align: left;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <h1>Listado de Medicamentos</h1>
  <table>
    <thead>
      <tr>
        <th>Código Medicamento</th>
        <th>Descripción</th>
        <th>Fecha Fabricación</th>
        <th>Fecha Vencimiento</th>
      </tr>
    </thead>
    <tbody>
      <% medicamentos.forEach(medicamento => { %>
        <tr>
          <td><%= medicamento.CodMedicamento %></td>
          <td><%= medicamento.descripcionMed %></td>
          <td><%= medicamento.fechaFabricacion %></td>
          <td><%= medicamento.fechaVencimiento %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
